        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <form method="POST" action="/tickets">
                        <div class="form-group">
                            <label for="email">Địa chỉ Mail</label>
                            <input type="email" class="lgx-input-form form-control" id="email" name="email" required
                                placeholder="timelapse@example.com" />
                        </div>

                        <div class="form-group">
                            <label for="fullname">Họ & tên</label>
                            <input type="text" class="lgx-input-form form-control" id="fullname" name="fullname"
                                required placeholder="Namikaze Naruto" />
                        </div>

                        <div class="form-group">
                            <label for="numberphone">Số điện thoại</label>
                            <input type="number" class="lgx-input-form form-control" id="numberphone" name="numberphone"
                                required placeholder="0982 654 767" />
                        </div>
                        <div class="form-group">
                            <label for="address">Địa chỉ nhận vé</label>
                            <input type="text" class="lgx-input-form form-control" id="" name="address"
                                required placeholder="LeParc Gamuda City, Hà Nội" />
                        </div>
                        <div class="form-group">
                            <label for="quanlity">Số lượng vé</label>
                            <input type="number" class="lgx-input-form form-control" id="quanlity" name="quanlity"
                                required placeholder="1" min="1" />
                        </div>

                        <div class="form-group">
                            <label for="exampleFormControlSelect1">Loại vé</label>
                            <select class="form-control" id="exampleFormControlSelect1" name="ticketId" required>

                                <% ticketCategories.map((item, index) => { %>
                                <option value="<%- item._id %>"><%- item.name %> (<%- item.prices %>.000 VND)</option>
                                <% }) %>
                            </select>
                        </div>
                        <div class="">
                            <p style=" font-weight: 500; color: #ea0202; font-family: initial; ">Sau khi đăng kí vui lòng kiểm tra Email để biết thêm về các phương thức thanh toán</p>
                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">
                                Cancel
                            </button>
                            <button type="submit" class="btn btn-primary"
                                style="    background: #ffc420; color: #4a25aa; border-color: #ffc420">
                                Confirm
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
